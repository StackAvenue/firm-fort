<%= stylesheet_link_tag 'style.css' %>
<div class="user">
  <div class="col-md-12">
    <h4>Search Results</h4>
  </div>
  <% if @users.any? %>
    <%= render '/users/shared/users_list' %>
    <%= render '/users/shared/pagination' %>
  <% else %>
    <div class="alert alert-warning" role="alert">
      No Results Found!!
    </div>
  <% end %>
</div>
